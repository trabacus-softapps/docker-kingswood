                </field>
            </field>
        </record>
        
        
        <!--    *****************************************************************************
                                Delivery  
       **************************************************************************** -->
        
        <record id="pr_view_picking_withcarrier_out_form_shipping" model="ir.ui.view">
            <field name="name">pr.view.picking.withcarrier.out.form.shipping</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form_shipping"/>
