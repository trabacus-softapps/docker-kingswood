            <field name="name">prova.delivery.stock.picking_withcarrier.form.view</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form"/>
            <field name="arch" type="xml">
            </field>
        </record>-->
                
        <!--    *****************************************************************************
                               Stock Transfer Details 
       **************************************************************************** -->
        
        <record id="pr_view_stock_enter_transfer_details" model="ir.ui.view">
            <field name="name">Enter transfer details</field>
            <field name="model">stock.transfer_details</field>
            <field name="inherit_id" ref="stock.view_stock_enter_transfer_details"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='quantity']" position="attributes">
                    <attribute name="readonly">0</attribute>
                </xpath>
            </field>
        </record>
        
        <!--    *****************************************************************************
                               Stock Picking IN Search
       **************************************************************************** -->
        
         <record id="pr_view_picking_internal_search" model="ir.ui.view">
