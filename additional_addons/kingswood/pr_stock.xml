       **************************************************************************** -->
        <record id="pr_view_move_picking_tree" model="ir.ui.view">
            <field name="name">pr.stock.move.tree</field>
            <field name="model">stock.move</field>
            <field eval="4" name="priority"/>
            <field name="inherit_id" ref="stock.view_move_picking_tree"/>
            <field name="arch" type="xml">
                <field name="state" position="before">
                    <field name="quality_id"/>
                    <field name="no_quality" attrs="{'readonly':[('quality_id','!=',False)]}"/>
                </field>
            </field>
        </record>
        
        
        
        <!--    *****************************************************************************
                                Stock move  - > Search
       **************************************************************************** -->
        <record id="pr_view_move_search" model="ir.ui.view">
            <field name="name">pr.stock.move.search</field>
            <field name="model">stock.move</field>
            <field eval="3" name="priority"/>
            <field name="inherit_id" ref="stock.view_move_search"/>
            <field name="arch" type="xml">
                <field name="origin" position="before">
                    <field name="quality_id"/>
