        <!--    *****************************************************************************
              Stock Form Inherit -> To hide create invoice button based on condition
       **************************************************************************** -->
        <record id="pr_view_picking_inherit_form2" model="ir.ui.view">
            <field name="name">pr.stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock_account.view_picking_inherit_form2"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@string='Create Invoice']" position="attributes">
                    <attribute name="attrs">{'invisible': ['|','|',('state','&lt;&gt;','done'),('origin_type','=','sample'),('invoice_state','&lt;&gt;','2binvoiced')]}</attribute>
                </xpath> 
         
            </field>
        </record>
        
        <!--    *****************************************************************************
              Searial number Form Inherit -> Show History 
       **************************************************************************** -->        

        <record id="pr_view_production_lot_form" model="ir.ui.view">
            <field name="name">pr.stock.production.lot.form</field>
            <field name="model">stock.production.lot</field>
             <field name="inherit_id" ref="stock.view_production_lot_form"/>    
            <field name="arch" type="xml">
                
                <xpath expr="//field[@name='name']" position="attributes">
                    <attribute name="on_change">onchange_name(name, context)</attribute>
                    <attribute name="help">"Unique Lot / Serial Number, format : 'yymmdd'  for ex : if the date is 2015-05-01 then the number will be  150501"</attribute>
                </xpath>
                
                <xpath expr="//notebook" position="after">
                    <div class="oe_chatter">
						<field name="message_ids" widget="mail_thread"/>
						<!-- <field name="message_follower_ids" widget="mail_followers"/> -->
					</div>
                </xpath> 
         
            </field>
        </record>

        <!--    *****************************************************************************
                     Picking Type - > Search View 
       **************************************************************************** -->        
    
        <record id="pr_view_pickingtype_filter" model="ir.ui.view">
            <field name="name">pr.stock.picking.type.filter</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_pickingtype_filter"/>
            <field name="arch" type="xml">
                <field name="warehouse_id" position="after">
                    <group expand="0" string="Group By">
                        <filter name="warehouse" string="Warehouse" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'warehouse_id'}"/>
                    </group>
                </field>
            </field>
        </record>
        
        <record id="stock.action_picking_type_form" model="ir.actions.act_window">
            <field name="context">{'search_default_warehouse':1}</field>
        </record>
        
        <!--    *****************************************************************************
                             Stock Production Lot -> Action
       **************************************************************************** -->     
